{
    "getAllTickets": {
        "schema": {
            "method": "GET",
            "host": "<%= context.host %>",
            "path": "/api/v2/tickets",
            "headers": {
                "Authorization": "Basic <%= encode(context.api + ':X') %>",
                "Content-Type": "application/json"
            }
        }
    },

    "login":{
        "schema":{
            "method": "POST",
            "host": "<%= context.base_url %>",
            "path": "/login"
        }
    },

    "signup": {
        "schema": {
            "method": "POST",
            "host": "<%= context.base_url %>",
            "path": "/sign_up"
        }
    },

    "getAppSchema": {
        "schema": {
            "method": "GET",
            "host": "<%= context.base_url %>",
            "path": "/apps/123"
        }
    }
}